<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Login.Ello">
    <ContentPage.Content>
        <StackLayout>
            <Frame>
                <Label Text="Hello" FontSize="Title"></Label>
            </Frame>
            <Label Text="Title:" FontSize="Medium" Margin="10,0,0,0"></Label>
            <Label x:Name="TitleHello" FontSize="Medium" Text="Title" Margin="10,0,0,0"></Label>
            <Label Text="Name:" FontSize="Medium" Margin="10,0,0,0"></Label>
            <Label x:Name="NameHello" FontSize="Medium" Text="Name" Margin="10,0,0,0"></Label>
            <Label Text="Password:" FontSize="Medium" Margin="10,0,0,0"></Label>
            <Label x:Name="PassHello" FontSize="Medium" Text="Password" Margin="10,0,0,0"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
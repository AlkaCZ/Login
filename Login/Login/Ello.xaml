<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Login.Ello">
    <ContentPage.Content>
        <StackLayout>
            <Frame>
                <Label Text="Hello" FontSize="Title"></Label>
            </Frame>
            <Label x:Name="TitleHello" Text="Title"></Label>
            <Label x:Name="NameHello" Text="Name"></Label>
            <Label x:Name="PassHello" Text="Password"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>